{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "maxNodeModuleJsDepth": 0,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "jsx": "react-native"
  },
  "include": ["*"]
}
