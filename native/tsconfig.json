{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "incremental": true,
    "noImplicitAny": true
  },
  "exclude": ["node_modules/**/*", "../node_modules", "expo"]
}
