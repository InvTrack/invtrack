{
  "name": "invtrack",
  "version": "1.3.2",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "cd .. && husky install",
    "precommit": "lint-staged",
    "lint": "eslint --fix --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "gen-types": "cp ../database.types.ts db/types/generated.ts",
    "eas:update:staging": "eas update --branch staging --message --",
    "eas:update:production": "eas update --branch production --message --",
    "eas:build:staging": "eas build -p all --profile staging",
    "eas:build:production": "eas build -p all --profile production"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/config-plugins": "~7.8.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "6.0.2",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@supabase/supabase-js": "2.8.0",
    "@tanstack/query-async-storage-persister": "4.18.0",
    "@tanstack/react-query": "4.18.0",
    "@tanstack/react-query-persist-client": "4.18.0",
    "date-fns": "^2.30.0",
    "expo": "~50.0.7",
    "expo-camera": "~14.0.5",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
    "expo-screen-orientation": "~6.4.1",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.11",
    "expo-web-browser": "~12.8.2",
    "lodash": "^4.17.21",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.73.4",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-url-polyfill": "^1.3.0",
    "expo-application": "~5.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.16.0",
    "@types/react": "~18.2.45",
    "eslint": "^8.35.0",
    "eslint-config-universe": "^11.2.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.2",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react-responsive": "^9.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
