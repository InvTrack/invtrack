{
  "name": "invtrack",
  "version": "1.5.2",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prepare": "cd .. && husky install",
    "precommit": "lint-staged",
    "lint": "eslint --fix --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "gen-types": "cp ../database.types.ts db/types/generated.ts",
    "eas:update:staging": "eas update --branch staging --message",
    "eas:update:production": "eas update --branch production --message",
    "eas:build:staging": "eas build -p all --profile staging",
    "eas:build:production": "eas build -p all --profile production"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/config-plugins": "~8.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "6.0.2",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "2.2.3",
    "@supabase/supabase-js": "2.38.4",
    "@tanstack/query-async-storage-persister": "4.18.0",
    "@tanstack/react-query": "4.18.0",
    "@tanstack/react-query-persist-client": "4.18.0",
    "date-fns": "^2.30.0",
    "expo": "51.0.8",
    "expo-application": "~5.9.1",
    "expo-camera": "~15.0.9",
    "expo-font": "~12.0.5",
    "expo-linking": "~6.3.1",
    "expo-screen-orientation": "~7.0.5",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-updates": "~0.25.14",
    "expo-web-browser": "~13.0.3",
    "lodash": "^4.17.21",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-redux": "9.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.16.0",
    "@types/react": "~18.2.79",
    "eslint": "^8.35.0",
    "eslint-config-universe": "^11.2.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.2",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.0"
  },
  "private": true
}
